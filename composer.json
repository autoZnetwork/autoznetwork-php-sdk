{
    "name": "autoznetwork/autoznetwork-php-sdk",
    "description": "autoZnetwork PHP SDK",
    "type": "library",
    "homepage": "https://github.com/autoznetwork/autoznetwork-php-sdk",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "bin\\": "bin/",
            "AutozNetwork\\": "src/",
            "AutozNetwork\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "collinped",
            "email": "collin@autoznetwork.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "saloonphp/saloon": "^3.0"
    },
    "require-dev": {
        "spatie/ray": "^1.41",
        "laravel/pint": "^1.17",
        "nette/utils": "^4.0",
        "phpstan/phpstan": "^1.12",
        "pestphp/pest": "^2.35"
    },
    "scripts": {
        "test": [
            "./vendor/bin/pest"
        ],
        "fix-code": [
            "./vendor/bin/pint"
        ]
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
